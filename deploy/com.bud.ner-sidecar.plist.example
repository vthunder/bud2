<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bud.ner-sidecar</string>

    <key>ProgramArguments</key>
    <array>
        <string>@BUD_DIR@/sidecar/run.sh</string>
        <string>--port</string>
        <string>8099</string>
    </array>

    <key>WorkingDirectory</key>
    <string>@BUD_DIR@/sidecar</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>@HOME@/Library/Logs/ner-sidecar.log</string>

    <key>StandardErrorPath</key>
    <string>@HOME@/Library/Logs/ner-sidecar.log</string>

    <key>ProcessType</key>
    <string>Background</string>
</dict>
</plist>
