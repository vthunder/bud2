<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bud.daemon</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/thunder/src/bud2/deploy/run-bud.sh</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/thunder/src/bud2</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/Users/thunder/.local/bin:/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
        <key>HOME</key>
        <string>/Users/thunder</string>
        <key>TERM</key>
        <string>xterm-256color</string>
        <key>BUD_SERVICE</key>
        <string>1</string>
    </dict>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/dev/null</string>

    <key>StandardErrorPath</key>
    <string>/dev/null</string>

    <key>ProcessType</key>
    <string>Background</string>
</dict>
</plist>
